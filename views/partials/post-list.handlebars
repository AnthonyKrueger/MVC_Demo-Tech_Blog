<div class="row">
    <a class="col s12 card indigo lighten-1 postEntry" href="/post/{{id}}">
        <div class="card-content white-text">
            <span class="card-title">{{title}} - {{user.username}}</span>
            <pre>{{text}}</pre>
            <p class="right">{{format_time updatedAt}}</p>
        </div>
    </a>
</div>
